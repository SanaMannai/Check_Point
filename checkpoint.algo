
ALGORITHM insertion_sort 

VAR
    i,j:INTEGER;
    arr:INTEGER;
    n:INTEGER:=length-1;

BEGIN

 FOR i FROM 0 TO arr.length-1 STEP 1 DO
    n:= arr[i]
 j=i-1;
  WHILE (arr[j] > n AND j >=0) DO
  arr[j+1] := arr[j];
  j:=j-1
  END_WHILE
  arr[j+1] := n
 END_FOR
    
END



































